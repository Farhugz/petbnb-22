<%= simple_form_for(@pet_home) do |f| %>
  <%= f.input :location %>
  <%= f.input :pet_type %>
  <%= f.input :responsibilities %>
  <%= f.input :capacity%>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :emergency_info %>
  <%= f.input :start_date %>
  <%= f.input :end_date %>
  <%= f.submit "Submit", class: "btn btn-primary" %>
<% end %>
